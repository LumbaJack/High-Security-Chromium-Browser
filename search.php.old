function pagerPlus() {
	if(cpage < pagect)
	{
		cpage++;
		//pageChange(cpage);
		pageJump(cpage);
	}
}

function pagerChange(ipage) {
	pageJump(ipage);
}

function pageJump(ipage) {

	if(ipage != 1)
	{
		doWait();
		window.setTimeout('doJump(' + ipage + ');',1000);
	}
	else
		doJump(ipage);
		
}

function doWait() {
	$("#loading").css({"height": $(document).height()+"px"});
	$("#loading").show();
	document.body.style.cursor = "wait";
}

function doJump(ipage) {
	
	if(ipage > 0 && ipage <= pagect)
	{	
		for(ii = 1; ii < (pagect+1); ii++)
		{
			$("#page" + ii).hide();
			$("#pagerBtn" + ii).removeClass('active');
		}
		
		$("#page" + ipage).show();
		
		$("#pagerBtn" + ipage).addClass('active');
		
		// page range
		rstart = ((ipage-1) * 10) + 1;
		rend = (ipage * 10);
		
		if(ipage == pagect)
		{
			$("#pagerNext").hide();	
			
			if((tresults % 10) != 0)
				rend = (tresults % 10) + (rstart-1);
			
		}
		else
			$("#pagerNext").show();
			
		// almost done, set global current page var	
		cpage = ipage;
				
		$("#rStart").text(rstart);	
		$("#rEnd").text(rend);
		
		window.scroll(0,0);
		
	}
	
	$("#loading").hide();
	document.body.style.cursor = "default";
}
